{
  "name": "unplugin-atscript",
  "version": "0.1.22",
  "description": "Atscript: Configuration and build plugins.",
  "keywords": [
    "annotations",
    "atscript",
    "unplugin",
    "vite-plugin",
    "rollup-plugin",
    "rolldown-plugin",
    "webpack-plugin",
    "esbuild-plugin",
    "rspack-plugin"
  ],
  "homepage": "https://github.com/moostjs/atscript/tree/main/packages/unplugin#readme",
  "bugs": {
    "url": "https://github.com/moostjs/atscript/issues"
  },
  "license": "ISC",
  "author": "Artem Maltsev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moostjs/atscript.git",
    "directory": "packages/unplugin"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./rolldown": {
      "types": "./dist/rolldown.d.ts",
      "import": "./dist/rolldown.mjs",
      "require": "./dist/rolldown.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./farm": {
      "types": "./dist/farm.d.ts",
      "import": "./dist/farm.mjs",
      "require": "./dist/farm.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "pub": "pnpm publish --access public",
    "test": "vitest"
  },
  "dependencies": {
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@atscript/core": "workspace:^",
    "@atscript/typescript": "workspace:^"
  },
  "build": [
    {},
    { "entries": ["src/vite.ts"] },
    { "entries": ["src/rollup.ts"] },
    { "entries": ["src/rolldown.ts"] },
    { "entries": ["src/webpack.ts"] },
    { "entries": ["src/esbuild.ts"] },
    { "entries": ["src/rspack.ts"] },
    { "entries": ["src/farm.ts"] }
  ]
}
