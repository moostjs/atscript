{
  "name": "@anscript/typescript",
  "version": "0.0.0",
  "description": "Anscript: typescript-gen support.",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "build": {
    "entries": [
      "src/index.ts",
      "src/runtime.ts"
    ],
    "dts": true
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest"
  },
  "keywords": [
    "anscript",
    "annotations",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anscript/core": "workspace:^"
  },
  "devDependencies": {
    "vitest": "^2.1.8"
  }
}
