{
  "name": "@ts-anscript/typescript",
  "version": "0.0.4",
  "description": "Anscript: typescript-gen support.",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "cli.cjs"
  ],
  "bin": {
    "asc": "./cli.cjs"
  },
  "build": [
    {},
    {
      "format": "cjs",
      "entries": [
        "src/cli/cli.ts"
      ],
      "dts": false
    }
  ],
  "scripts": {
    "pub": "pnpm publish --access public",
    "test": "vitest"
  },
  "keywords": [
    "anscript",
    "annotations",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ts-anscript/core": "workspace:^",
    "@moostjs/event-cli": "^0.5.21",
    "moost": "^0.5.21"
  },
  "devDependencies": {
    "vitest": "^3.0.0"
  }
}
